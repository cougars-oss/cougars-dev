{
  "name": "CoUGARs Dev",
  "dockerComposeFile": "docker-compose.yaml",
  "service": "dev",
  "workspaceFolder": "/home/frostlab-docker/ros2_ws/src",
  "remoteUser": "frostlab-docker",
  "updateRemoteUserUID": true,
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-iot.vscode-ros",
        "ms-vscode.cpptools",
        "ms-python.python",
        "twxs.cmake",
        "eamodio.gitlens",
        "redhat.vscode-xml",
        "yzhang.markdown-all-in-one"
      ]
    }
  },
  "postCreateCommand": [
    "/bin/bash",
    ".devcontainer/postCreateCommand.sh"
  ]
}