{
    "name": "coug_openwater",
    "package_name": "Ocean",
    "world": "OpenWater",
    "main_agent": "auv0",
    "ticks_per_sec": 200,
    "frames_per_sec": false,
    "agents": [
        {
            "agent_name": "auv0",
            "agent_type": "CougUV",
            "sensors": [
                {
                    "sensor_type": "IMUSensor",
                    "socket": "IMUSocket",
                    "Hz": 200,
                    "configuration": {
                        "AccelSigma": 0.0078,
                        "AngVelSigma": 0.0012,
                        "AccelBiasSigma": 1.05e-05,
                        "AngVelBiasSigma": 3.91e-06,
                        "ReturnBias": true
                    },
                    "ros_publish": true
                },
                {
                    "sensor_type": "GPSSensor",
                    "socket": "COM",
                    "Hz": 1,
                    "configuration": {
                        "Sigma": 0.015,
                        "Depth": 0.5,
                        "DepthSigma": 0.025
                    },
                    "ros_publish": true
                },
                {
                    "sensor_type": "DepthSensor",
                    "socket": "DepthSocket",
                    "Hz": 10,
                    "configuration": {
                        "Sigma": 0.02
                    },
                    "ros_publish": true
                },
                {
                    "sensor_type": "MagnetometerSensor",
                    "socket": "IMUSocket",
                    "Hz": 100,
                    "configuration": {
                        "Sigma": 0.003,
                        "MagneticVector": [
                            0.07857,
                            0.41316,
                            -0.90726
                        ]
                    },
                    "ros_publish": true
                },
                {
                    "sensor_type": "RotationSensor",
                    "socket": "IMUSocket",
                    "Hz": 10,
                    "ros_publish": true
                },
                {
                    "sensor_type": "DVLSensor",
                    "socket": "DVLSocket",
                    "Hz": 15,
                    "configuration": {
                        "VelSigma": 0.02,
                        "DebugLines": false
                    },
                    "ros_publish": true
                },
                {
                    "sensor_type": "DynamicsSensor",
                    "configuration": {
                        "UseCOM": true,
                        "UseRPY": false
                    },
                    "ros_publish": true
                },
                {
                    "sensor_type": "VelocitySensor",
                    "socket": "DVLSocket",
                    "ros_publish": true
                }
            ],
            "fossen_model": "torpedo",
            "control_mode": "depthHeadingAutopilot",
            "control_scheme": 1,
            "location": [
                -0.1,
                0,
                -0.1
            ],
            "rotation": [
                0,
                0,
                0
            ],
            "actuator": {
                "fin_count": 3,
                "fin_offset_deg": 90.0
            }
        }
    ]
}
