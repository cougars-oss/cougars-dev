<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>coug_bringup</name>
  <version>0.0.0</version>
  <description>Full-system launch files for the CougUV</description>
  <maintainer email="snelsondurrant@gmail.com">Nelson Durrant</maintainer>
  <license>Apache-2.0</license>

  <exec_depend>coug_description</exec_depend>
  <exec_depend>coug_fgo</exec_depend>
  <exec_depend>coug_localization</exec_depend>
  <exec_depend condition="$ROS_BUILD_ARCH == 'amd64'">coug_mapviz</exec_depend>
  <exec_depend>coug_navigation</exec_depend>
  <exec_depend>coug_rqt</exec_depend>
  <exec_depend>coug_rviz</exec_depend>
  <exec_depend>holoocean_bridge</exec_depend>
  <exec_depend>sensor_bridge</exec_depend>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
